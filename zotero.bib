  file = {/home/pierre/Zotero/storage/CDME3Q55/Yang et al. - 2017 - Application of optimal transport and the quadratic.pdf;/home/pierre/Zotero/storage/G7UWAH49/Yang et al. - 2017 - Application of optimal transport and the quadratic.pdf;/home/pierre/Zotero/storage/QHYL2WXQ/Yang et al. - 2017 - Application of Optimal Transport and the Quadratic.pdf;/home/pierre/Zotero/storage/LENG44KS/geo2016-0663.html;/home/pierre/Zotero/storage/QCVUKC6F/geo2016-0663.html},
