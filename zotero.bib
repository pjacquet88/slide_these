@article{alauzet3DTransientFixed2007,
  title = {{{3D}} Transient Fixed Point Mesh Adaptation for Time-Dependent Problems: {{Application}} to {{CFD}} Simulations},
  shorttitle = {{{3D}} Transient Fixed Point Mesh Adaptation for Time-Dependent Problems},
