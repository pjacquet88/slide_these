@article{ainsworthPyramidAlgorithmsBernstein2014,
  ids = {ainsworthPyramidAlgorithmsBernstein2014a},
  title = {Pyramid {{Algorithms}} for {{Bernstein}}--{{B\'ezier Finite Elements}} of {{High}}, {{Nonuniform Order}} in {{Any Dimension}}},
