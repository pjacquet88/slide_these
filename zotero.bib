@article{stanglmeierExplicitHybridizableDiscontinuous2016,
  title = {An Explicit Hybridizable Discontinuous {{Galerkin}} Method for the Acoustic Wave Equation},
  author = {Stanglmeier, M. and Nguyen, N. C. and Peraire, J. and Cockburn, B.},
  year = {2016},
  month = mar,
  volume = {300},
  pages = {748--769},
  issn = {0045-7825},
  doi = {10.1016/j.cma.2015.12.003},
  abstract = {We present an explicit hybridizable discontinuous Galerkin (HDG) method for numerically solving the acoustic wave equation. The method is fully explicit, high-order accurate in both space and time, and coincides with the classic discontinuous Galerkin (DG) method with upwinding fluxes for a particular choice of its stabilization function. This means that it has the same computational complexity as other explicit DG methods. However, just as its implicit version, it provides optimal convergence of order k+1 for all the approximate variables including the gradient of the solution, and, when the time-stepping method is of order k+2, it displays a superconvergence property which allow us, by means of local postprocessing, to obtain new improved approximations of the scalar field variables at any time levels for which an enhanced accuracy is required. In particular, the new approximations converge with order k+2 in the L2-norm for k{$\geq$}1. These properties do not hold for all numerical fluxes. Indeed, our results show that, when the HDG numerical flux is replaced by the Lax\textendash Friedrichs flux, the above-mentioned superconvergence properties are lost, although some are recovered when the Lax\textendash Friedrichs flux is used only in the interior of the domain. Finally, we extend the explicit HDG method to treat the wave equation with perfectly matched layers. We provide numerical examples to demonstrate the performance of the proposed method.},
  file = {/home/pierre/Zotero/storage/XYAC6JNU/Stanglmeier et al. - 2016 - An explicit hybridizable discontinuous Galerkin me.pdf;/home/pierre/Zotero/storage/4TJM3JZY/S0045782515003941.html},
  journal = {Computer Methods in Applied Mechanics and Engineering},
  keywords = {Discontinuous Galerkin methods,Finite element method,Runge–Kutta methods,Superconvergence,Wave equation},
