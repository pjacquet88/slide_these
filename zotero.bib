  file = {/home/pierre/Zotero/storage/LWK8N59T/Wang et al. - 2011 - On 3D modeling of seismic wave propagation via a s.pdf;/home/pierre/Zotero/storage/RQ8MCR6N/Wang et al. - 2011 - On 3D modeling of seismic wave propagation via a s.pdf;/home/pierre/Zotero/storage/ADBC3SAA/j.1365-2478.2011.00982.html},
