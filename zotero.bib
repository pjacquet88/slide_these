  file = {/home/pierre/Zotero/storage/L4VZI4KW/Szenicer et al. - 2020 - A complexity-driven framework for waveform tomogra.pdf;/home/pierre/Zotero/storage/3WASHDUC/9267723.html;/home/pierre/Zotero/storage/PQNLZGYH/9267723.html},
