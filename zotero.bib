@article{cockburnDevisingDiscontinuousGalerkin2001,
  title = {Devising Discontinuous {{Galerkin}} Methods for Non-Linear Hyperbolic Conservation Laws},
  author = {Cockburn, Bernardo},
  year = {2001},
  month = mar,
  volume = {128},
